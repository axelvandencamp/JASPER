-- Function: _crm_ledenviaafdeling(text, date)

-- DROP FUNCTION _crm_ledenviaafdeling(text, date);

CREATE OR REPLACE FUNCTION _crm_ledenviaafdeling(
    IN lidnummer text,
    IN begindatum date,
    OUT id integer,
    OUT naam character varying,
    OUT afdeling character varying)
  RETURNS SETOF record AS
$BODY$
BEGIN
	RETURN QUERY 
	SELECT p.id, p.name, a5.name afdeling 
	FROM res_partner p
		INNER JOIN res_partner a5 ON COALESCE(p.recruiting_organisation_id,0) = a5.id
	WHERE COALESCE(p.recruiting_organisation_id,0) > 0 AND p.create_date > begindatum;

	 
END; 
$BODY$
  LANGUAGE plpgsql VOLATILE
  COST 100
  ROWS 1000;
ALTER FUNCTION _crm_ledenviaafdeling(text, date)
  OWNER TO odbcreadonly;
